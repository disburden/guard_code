    "version": "0.2.0",
    "configurations": [
        {
            "name": "guard_code (Web)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}",
            "deviceId": "chrome",
            "toolArgs": [
                "--web-port=5531",
                "--web-hostname=localhost",
                "--web-browser-flag=--user-data-dir=/tmp/chrome_data"
            ]
        },
        {
            "name": "guard_code (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "guard_code (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}